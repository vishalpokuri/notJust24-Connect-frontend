{
  "expo": {
    "name": "Connect-jsm",
    "slug": "Connect-jsm",
    "scheme": "connect",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.vishal.connections"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": ["expo-router", "expo-font", "expo-camera"],

    "experiments": {
      "typedRoutes": true
    }
  }
}
